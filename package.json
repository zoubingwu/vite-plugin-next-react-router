{
  "name": "vite-plugin-next-react-router",
  "version": "0.7.1",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "fmt": "prettier src/ --write",
    "example:dev": "npm --prefix examples/react run start",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "consola": "^3.1.0",
    "fast-glob": "^3.2.12",
    "react-router-dom": "^6.10.0"
  },
  "peerDependencies": {
    "vite": ">=2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "directory-tree": "^3.5.1",
    "husky": "^8.0.3",
    "lodash-es": "^4.17.21",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.2",
    "vitest": "^0.30.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubingwu/vite-plugin-next-react-router.git"
  },
  "bugs": {
    "url": "https://github.com/zoubingwu/vite-plugin-next-react-router/issues"
  },
  "homepage": "https://github.com/zoubingwu/vite-plugin-next-react-router",
  "sideEffect": false,
  "files": [
    "dist",
    "src",
    "client.d.ts"
  ],
  "keywords": [
    "vite-plugin",
    "react-router",
    "router",
    "react"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
